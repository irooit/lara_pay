{
    "name": "tintsoft/laravel-skeleton",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "ext-bcmath": "*",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-openssl": "*",
        "ext-simplexml": "*",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "5.7.*",
        "laravel/passport": "^7.0",
        "laravel/tinker": "^1.0",
        "predis/predis": "^1.1",
        "socialiteproviders/qq": "^3.0",
        "socialiteproviders/weibo": "^3.0",
        "socialiteproviders/weixin": "^4.0",
        "socialiteproviders/weixin-web": "^3.0",
        "xutl/flysystem-aliyun-oss": "^1.0",
        "xutl/laravel-aliyun": "^1.2",
        "xutl/laravel-aliyun-mns-queue": "^1.0",
        "xutl/laravel-passport-sms": "^1.0",
        "xutl/laravel-passport-socialite": "^1.0",
        "xutl/laravel-sms": "^1.0",
        "xutl/supports": "^1.0",
	"yansongda/laravel-pay": "^2.0"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "autoloader-suffix": "_forge",
        "notify-on-install": false
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php artisan passport:keys --ansi",
            "@php artisan storage:link --ansi"
        ]
    }
}
